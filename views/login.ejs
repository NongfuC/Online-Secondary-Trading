<% include header %>
<div class="container">
<div class="row">
<div class="col-md-4"></div>
<div class="col-md-4">
  <h1>登录</h1>
  <div class="login">
    <p class="input-group">
      <label class="input-group-addon" for="uname"><span class="glyphicon glyphicon-user"></span></label>
      <input id="uname" class="form-control uname" type="text" placeholder="用户名">
    </p>
    <p class="input-group">
      <label class="input-group-addon" for="password"><span class="glyphicon glyphicon-pencil"></span></label>
      <input id="password" class="form-control password" type="password" placeholder="密码">
    </p>
    <p class="input-group">
      <button class="btn btn-lg btn-success login-btn" type="button">登陆</button>
      &nbsp;&nbsp;<a class="btn btn-lg btn-primary" href="/reg">注册</a>
    </p>
  </div>
</div>
<div class="col-md-4"></div>
</div>
</div>
<% include footer %>
