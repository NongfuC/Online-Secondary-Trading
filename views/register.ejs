<% include header %>
<div class="container">
	<div class="col-md-4"></div>
	<div class="reg col-sm-4">
		<h1>注册</h1>
		<p class="input-group">
			<label class="input-group-addon" for="uname">
				<span class="glyphicon glyphicon-user"></span>
			</label>
			<input id="uname" class="form-control uname" type="text" placeholder="用户名">		
		</p>
		<p class="input-group">
			<label class="input-group-addon" for="password">
				<span class="glyphicon glyphicon-pencil"></span>
			</label>
			<input id="password" class="form-control password" type="password" placeholder="密码">
		</p>
		<p class="input-group">
			<label class="input-group-addon" for="qr_password">
				<span class="glyphicon glyphicon-pencil"></span>
			</label>
			<input id="qr_password" class="form-control qr_password" type="password" placeholder="再次输入密码">
		</p>
		<p class="input-group">
			<label class="input-group-addon" for="qr_password">
				<span class="glyphicon glyphicon-flag"></span>
			</label>
      <input id="school" name="school" class="form-control" type="text" placeholder="请填写完整的大学名称">
		</p>
		<p class="input-group">
			<button class="btn btn-lg btn-success reg-btn" type="button">注册</button>
		</p>
	</div>
	<div class="col-md-4"></div>
</div>
<% include footer %>